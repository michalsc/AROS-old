# $Id$
include $(TOP)/config/make.cfg

OS_INCLUDES	:= -I$(GENINCDIR)
OBJDIR		:= $(GENDIR)/filesys
FILES		:= emul_handler
OBJS		:= $(foreach f,$(FILES),$(OBJDIR)/$(f).o)
DEPS		:= $(foreach f,$(FILES),$(OBJDIR)/$(f).d)
LIB		:= $(OBJDIR)/emul_handler.a

#MM
kernel-filesys-$(ARCH)-$(CPU) : $(LIB)
	@$(NOP)

#MM kernel-filesys : setup includes kernel-filesys-$(ARCH)-$(CPU)
kernel-filesys :
	@$(NOP)

#MM
setup :
	%mkdirs_q $(OBJDIR)

#MM
clean ::
	-$(RM) $(OBJDIR) *.err

$(LIB) : $(OBJS)
	%mklib_q

$(OBJDIR)/%.o : %.c
	%compile_q

$(OBJDIR)/%.d : %.c
	%mkdepend_q

%common
%include_deps
